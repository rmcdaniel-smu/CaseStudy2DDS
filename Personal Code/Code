#Read in dataset
URL <- getURL("https://raw.githubusercontent.com/rmcdaniel-smu/CaseStudy2DDS/master/CaseStudy2-data.csv")
train <- read.csv(text = URL, sep = ',')
str(train)
ncol(train)
